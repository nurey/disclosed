application: govtenders2
version: 1
runtime: python
api_version: 1

default_expiration: '3650d'

handlers:
- url: /remote_api
  script: $PYTHON_LIB/google/appengine/ext/remote_api/handler.py
  login: admin
  
- url: /delete.*
  script: delete.py
  login: admin
  
- url: /static
  static_dir: static

- url: /media
  static_dir: _generated_media

- url: .*
  script: common/appenginepatch/main.py
